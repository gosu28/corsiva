<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="lazada_connector_ir_attachment_inherit_form_view" model="ir.ui.view">
            <field name="name">lazada_connector_ir_attachment_inherit_form_view</field>
            <field name="model">ir.attachment</field>
            <field name="inherit_id" ref="base.view_attachment_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='public']" position="after">
                    <field name="public_url"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>